<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<factory>
		<exception-handler-factory>
			pl.estrix.frontend.jsf.CustomExceptionHandlerFactory
		</exception-handler-factory>
	</factory>

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>

		<resource-bundle>
			<base-name>filter</base-name>
			<var>mainprop</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>links</base-name>
			<var>linkprop</var>
		</resource-bundle>

		<!--<locale-config>-->
			<!--<default-locale>pl</default-locale>-->
			<!--<supported-locale>en</supported-locale>-->
		<!--</locale-config>-->
		<!--<message-bundle>messages</message-bundle>-->
	</application>
	<lifecycle>
		<phase-listener>pl.estrix.frontend.jsf.common.MultiPageMessagesSupport</phase-listener>
	</lifecycle>

	<navigation-rule>
		<display-name>secured/event/index.xhtml</display-name>
		<from-view-id>/secured/event/index.xhtml</from-view-id>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/print/index.xhtml</display-name>
		<from-view-id>/secured/print/index.xhtml</from-view-id>
	</navigation-rule>

	<navigation-rule>
		<display-name>unsecured/policy.xhtml</display-name>
		<from-view-id>/unsecured/policy.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>main</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>policy</from-outcome>
			<to-view-id>/unsecured/policy.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/store/index.xhtml</display-name>
		<from-view-id>/secured/store/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>details</from-outcome>
			<to-view-id>/secured/store/details.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/store/details.xhtml</display-name>
		<from-view-id>/secured/store/details.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/store/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/store/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<display-name>secured/operator/index.xhtml</display-name>
		<from-view-id>/secured/operator/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>products</from-outcome>
			<to-view-id>/secured/operator/products.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>details</from-outcome>
			<to-view-id>/secured/operator/products.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/operator/products.xhtml</display-name>
		<from-view-id>/secured/operator/products.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/operator/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/operator/details.xhtml</display-name>
		<from-view-id>/secured/operator/details.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/operator/products.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/operator/products.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>secured/shipment/index.xhtml</display-name>
		<from-view-id>/secured/shipment/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>details</from-outcome>
			<to-view-id>/secured/shipment/details.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/secured/shipment/new.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/shipment/details.xhtml</display-name>
		<from-view-id>/secured/shipment/details.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/shipment/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/shipment/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/shipment/new.xhtml</display-name>
		<from-view-id>/secured/shipment/new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/shipment/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/shipment/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>secured/release/index.xhtml</display-name>
		<from-view-id>/secured/release/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>details</from-outcome>
			<to-view-id>/secured/release/details.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>secured/release/details.xhtml</display-name>
		<from-view-id>/secured/release/details.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/release/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/release/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>secured/user/index.xhtml</display-name>
		<from-view-id>/secured/user/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>details</from-outcome>
			<to-view-id>/secured/user/details.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>password</from-outcome>
			<to-view-id>/secured/user/change_password.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/secured/user/new.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/user/details.xhtml</display-name>
		<from-view-id>/secured/user/details.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/user/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/user/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secured/user/change_password.xhtml</display-name>
		<from-view-id>/secured/user/change_password.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saveAndOut</from-outcome>
			<to-view-id>/secured/user/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/secured/user/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

</faces-config>